      <label for"name">Name</label>
      <input 
        type="text" 
        name="name" 
        id="name" 
      />
      <label for="dept">Department</label>
      <input 
        input="text"
        name="dept"
        id="dept"
      />
      <button>
        Submit
      </button>
    </form>
  </body>
</html>